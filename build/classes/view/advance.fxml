                  <Insets right="20.0" top="5.0" />
               </HBox.margin>
            </Text>
            <Text fx:id="max" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" HBox.hgrow="ALWAYS">
