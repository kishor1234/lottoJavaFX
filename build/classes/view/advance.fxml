<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="pane" stylesheets="@../css/login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdvanceController">
   <children>
      <HBox prefHeight="30.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="How many draw selectd, enter number here" HBox.hgrow="ALWAYS">
